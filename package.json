{
  "name": "telegram-miniapp-backend",
  "version": "1.0.0",
  "description": "Backend API for Telegram Mini App",
  "main": "api/src/server.js",
  "type": "module",
  "engines": {
    "node": ">=20 <23"
  },
  "scripts": {
    "dev": "nodemon api/src/server.js",
    "dev:frontend": "vite --config miniapp/frontend/vite.config.ts --port 5173",
    "dev:bot": "PORT=3001 pnpm exec tsx src/index.ts",
    "start": "node api/src/server.js",
    "worker:keepalive": "node api/src/worker/keepalive.js",
    "build": "vite build",
    "lint": "eslint api/src",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "@twa-dev/sdk": "^7.0.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "better-sqlite3": "^12.6.2",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "googleapis": "^140.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.66.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.3.6",
    "zustand": "^4.4.7"
  },
  "optionalDependencies": {
    "canvas": "^3.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node-telegram-bot-api": "^0.64.10",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^15.15.0",
    "nodemon": "^3.0.2",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.46.0",
    "vite": "^5.0.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  }
}
